		    				<div class="diaporama11 span-9 block block-7-line" id="diaporama-11">
		    					<div class="block-content">
		    				{{ getSpipJquerySlider(idRubriqueBandeau, { "forceResize" : true, "width" : 400, theme : "Slider", "height" : 300, texte: false }) | raw }}
		    					</div>
		    				</div>
		    		</div>
		    		<div class="span-11 diaporama11-liste-2">
		    				<div class="texte-diaporama11 span-9 last block block-4-line" id="texte-casalini-1">
		    					{% for rubrique in boucleRubriques({ "idRubrique" : 11 }) %}						
									<div class="content">{{ rubrique.getTexte() | raw }}</div>
								{% endfor %}
									<div class="pointeur">
		    							<a class="pointeur11" href="vente-occasion-detail01">En savoir plus</a>
		    						</div>
		    				</div>
		    		</div>
