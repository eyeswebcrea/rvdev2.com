		    					<div class="block-content">
		    				{{ getSpipJquerySlider(idRubriqueBandeau, { "forceResize" : true, "width" : 400, "height" : 300, texte: false }) | raw }}
		    					</div>
		    				</div>
		    				<div class="texte-diaporama2 span-11 last block block-4-line" id="texte-casalini-1">
